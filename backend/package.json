{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "dev": "node --watch app.js",
    "generate": "dotenv -e .env -- npx prisma generate",
    "migrate:dev": "dotenv -e .env -- npx prisma migrate dev",
    "migrate:deploy": "dotenv -e .env -- npx prisma migrate deploy",
    "studio": "dotenv -e .env -- npx prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "prisma": "^6.18.0"
  },
  "devDependencies": {
    "dotenv-cli": "^11.0.0"
  }
}
